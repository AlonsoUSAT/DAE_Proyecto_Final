	<queryString>
		<![CDATA[SELECT
    p.nombre AS nombre_producto,
    tp.nombretipopresentacion AS tipo_presentacion,
    pr.cantidad AS cantidad_presentacion,
    u.nombreunidad AS unidad_medida,
    SUM(dv.cantidad) AS total_unidades_vendidas,
    COUNT(dv.iddetalle) AS num_ventas_con_producto
FROM
    public.detalle_venta dv
JOIN
    public.producto p ON dv.idproducto = p.idproducto
JOIN
    public.presentacion pr ON dv.idpresentacion = pr.idpresentacion
JOIN
    public.tipo_presentacion tp ON pr.tipopresentacion = tp.idtipopresentacion
JOIN
    public.unidad u ON pr.idunidad = u.idunidad
GROUP BY
    p.nombre,
    tp.nombretipopresentacion,
    pr.cantidad,
    u.nombreunidad
ORDER BY
    total_unidades_vendidas ASC]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT 
    p.nombre AS producto,
    m.nombre AS marca,
    l.nombrelaboratorio AS laboratorio,
    c.nombrecategoria AS categoria,
    tp.nombretipopresentacion AS presentacion,
    pp.stock,
    pp.precio
FROM public.presentacion_producto pp
INNER JOIN public.producto p ON pp.idproducto = p.idproducto
INNER JOIN public.marca m ON p.idmarca = m.idmarca
INNER JOIN public.laboratorio l ON m.idlaboratorio = l.idlaboratorio
INNER JOIN public.categoria c ON p.idcategoria = c.idcategoria
INNER JOIN public.presentacion pres ON pp.idpresentacion = pres.idpresentacion
INNER JOIN public.tipo_presentacion tp ON pres.tipopresentacion = tp.idtipopresentacion
WHERE pp.stock <= 10 
  AND p.estado = true
  AND ($P{p_idlaboratorio} IS NULL OR l.idlaboratorio = $P{p_idlaboratorio})
  AND ($P{p_idcategoria} IS NULL OR c.idcategoria = $P{p_idcategoria})
ORDER BY pp.stock ASC;]]>
	</queryString>

	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="fechaInicio" class="java.util.Date"/>
	<parameter name="fechaFin" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
    l.nombrelaboratorio AS "Laboratorio",
    COUNT(DISTINCT p.idproducto) AS "Productos Diferentes",
    SUM(dv.cantidad) AS "Unidades Vendidas",
    SUM(dv.cantidad * dv.precio) AS "Total Ingresos"
FROM
    public.detalle_venta dv
INNER JOIN public.venta v ON dv.idventa = v.idventa
INNER JOIN public.producto p ON dv.idproducto = p.idproducto
INNER JOIN public.marca m ON p.idmarca = m.idmarca
INNER JOIN public.laboratorio l ON m.idlaboratorio = l.idlaboratorio
WHERE
    v.estado = true
    AND v.fecha BETWEEN $P{fechaInicio} AND $P{fechaFin}
GROUP BY
    l.idlaboratorio, l.nombrelaboratorio
ORDER BY
    "Total Ingresos" DESC;]]>
	</queryString>
	<field name="Laboratorio" class="java.lang.String"/>
	<field name="Productos Diferentes" class="java.lang.Long"/>
	<field name="Unidades Vendidas" class="java.lang.Long"/>
	<field name="Total Ingresos" class="java.math.BigDecimal"/>

	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="66"/>
	<parameter name="fecha_inicio" class="java.util.Date"/>
	<parameter name="fecha_fin" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
    u.nomusuario AS "Usuario",
    CONCAT(u.nombres, ' ', u.apellidopaterno, ' ', COALESCE(u.apellidomaterno, '')) AS "Nombre Completo",
    r.nombre_rol AS "Rol",
    COUNT(c.idcomprobante) AS "Nro. Transacciones",
    SUM(c.total) AS "Total Recaudado"
FROM
    public.venta v
INNER JOIN
    public.usuario u ON v.idusuario = u.codusuario
INNER JOIN
    public.rol r ON u.id_rol = r.id_rol
INNER JOIN
    public.comprobante_venta c ON v.idventa = c.idventa
WHERE
    v.estado = true
    AND v.fecha >= $P{fecha_inicio}
    AND v.fecha <= $P{fecha_fin}
GROUP BY
    u.codusuario, u.nomusuario, u.nombres, u.apellidopaterno, u.apellidomaterno, r.nombre_rol
ORDER BY
    "Total Recaudado" DESC;]]>
	</queryString>

	<parameter name="fecha_inicio" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_fin" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    v.idVenta,
    v.fecha,
    v.hora,
    u.nomusuario AS vendedor_responsable,
    COALESCE(c.total, 0) AS monto_anulado,
    CASE
        WHEN v.estado = false THEN 'ANULADO'
        ELSE 'ACTIVO'
    END AS estado_venta
FROM VENTA v
JOIN USUARIO u ON v.idUsuario = u.codUsuario
LEFT JOIN COMPROBANTE_VENTA c ON v.idVenta = c.idVenta
WHERE v.fecha BETWEEN $P{fecha_inicio} AND $P{fecha_fin}
AND v.estado = false
ORDER BY v.fecha DESC, v.hora DESC;]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
    CASE
        WHEN EXISTS (SELECT 1 FROM public.persona p WHERE p.idcliente = c.idcliente) THEN 'Persona Natural'
        WHEN EXISTS (SELECT 1 FROM public.empresa e WHERE e.idcliente = c.idcliente) THEN 'Empresa'
        ELSE 'No Clasificado' 
    END AS tipo_cliente,
    COUNT(v.idventa) AS total_ventas,
    COUNT(DISTINCT v.idcliente) AS clientes_unicos
FROM
    public.venta v
JOIN
    public.cliente c ON v.idcliente = c.idcliente
GROUP BY
    tipo_cliente
ORDER BY
    total_ventas DESC;]]>
	</queryString>
	<field name="tipo_cliente" class="java.lang.String"/>
	<field name="total_ventas" class="java.lang.Long"/>

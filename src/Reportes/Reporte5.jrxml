	<parameter name="fecha_inicio" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_fin" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    u.nomusuario,
    u.nombres || ' ' || u.apellidoPaterno AS nombre_completo,
    r.nombre_rol,
    COUNT(c.idComprobante) AS numero_transacciones,
    SUM(c.total) AS total_recaudado
FROM VENTA v
JOIN USUARIO u ON v.idUsuario = u.codUsuario
JOIN ROL r ON u.id_rol = r.id_rol
JOIN COMPROBANTE_VENTA c ON v.idVenta = c.idVenta
WHERE v.fecha BETWEEN $P{fecha_inicio} AND $P{fecha_fin}
AND c.estado = true
GROUP BY u.codUsuario, u.nomusuario, u.nombres, u.apellidoPaterno, r.nombre_rol
ORDER BY total_recaudado DESC;]]>
	</queryString>
